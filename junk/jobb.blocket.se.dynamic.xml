<?xml version="1.0" encoding="utf-8"?>
<jobfeed_xml_structure>
  <!--XML structure for ad posted to StepStone and Blocket Jobb.
If you are in need of support please email “import@blocketjobb.se”.-->
<ads>
  <ad>
    <!-- Mandatory, (external ad id) or (unique ad reference number) -->
    <external_ad_id><![CDATA[7018]]></external_ad_id>

    <!-- Mandatory, target Job board
    Value "jobb"       :list on blocket jobb
    Value "stepstone"  :list on stepstone
    Value "combo"      :list on both
    -->
    <display_at><![CDATA[stepstone]]></display_at>

    <!--
    Mandatory, <categories> and /or <subcategories>, see our category list below.
    Maximum, 3 main <categories> and 3 <subcategories> per main category is allowed
    -->
    <category cgID="9230"/>
    <category cgID="9250">
      <subcategory scgID="9252"/>
    </category>
    <category cgID="9310">
      <subcategory scgID="9311"/>
      <subcategory scgID="9312"/>
    </category>

    <!-- If you use category_name you are allowed to enter any category string that we will mapp to our categories.-->
    <category_name><![CDATA[special category]]></category_name>

    <!-- Mandatory, Jobb ad titel, -->
    <subject><![CDATA[It Technician]]></subject>

    <!-- Mandatory -->
    <body><![CDATA[Lorem ipsum dolor sit amet, consectetur adipiscing elit. Maecenas eget ipsum ullamcorper, consectetur augue ut, rutrum nisl. Sed viverra metus ac tristique rutrum. Nunc imperdiet condimentum risus pulvinar porttitor. Morbi nec odio lacinia, vulputate dolor id, varius risus. Praesent odio erat, accumsan in quam a, dictum ultrices risus. Class aptent taciti sociosqu ad litora torquent per conubia nostra, per inceptos himenaeos. Nulla vel lorem quam. Proin eros massa, ultrices feugiat lacus quis, blandit hendrerit arcu. Maecenas sed nisl ac ex imperdiet porta. Aliquam et auctor augue. Ut ut elit turpis. Aenean faucibus arcu id eros tincidunt, quis pulvinar odio luctus. Vivamus nisl sem, varius auctor velit vel, porttitor consectetur lacus. Integer aliquet nisl non ante blandit suscipit. Vestibulum ante ipsum primis in faucibus orci luctus et ultrices posuere cubilia Curae; Suspendisse vel dui eu mi dictum aliquet.]]></body>

    <!--
    Mandatory, defaults to 1 if value is missing
    See our Category <job_ad_types> values below.
    -->
    <job_ad_type><![CDATA[1]]></job_ad_type>


    <!--
    Mandatory, defaults to 1 if value is missing
    See our Category <employment_list> values below.
    -->
    <employment><![CDATA[ 1 ]]></employment>
    <employment><![CDATA[ 2 ]]></employment>
    <employment><![CDATA[ 6 ]]></employment>
    <employment><![CDATA[ 12 ]]></employment>

    <!-- Optional, defaults to 1 if value is missing-->
    <job_count><![CDATA[1]]></job_count>

    <!-- Mandatory, Ad end date, YYYY-MM-DD (MAX 60 days into the future, MIN 2 days from NOW) -->
    <apply_date><![CDATA[2015-01-30]]></apply_date>

    <!--
    Mandatory,
    conditions: Only 1 region and 1 municipality allowed
    <region> Optional
    <region>+<municipalities> Mandatory
    <region>+<municipalities>+<region_name> (Optional - Highly recommended for better quality)
    -->
    <region><![CDATA[11]]></region>
    <municipality><![CDATA[131]]></municipality>
    <region_name><![CDATA[Stockholms Stad]]></region_name>

    <!-- Optional -->
    <adress><![CDATA[Company Adress]]></adress>

    <!-- Optional -->
    <zipcode><![CDATA[16474]]></zipcode>

    <!-- Mandatory -->
    <!-- This node is visible in the listing as recruiting company.
    End customer name or Recruiting company.
    This node can be used to filter out and redirect an end customer from the main VAT number into a different account with their own profile.-->
    <corp_name><![CDATA[Company name]]></corp_name>

    <!-- Mandatory, company VAT number -->
    <!-- Vat Number = Recruiting company VAT number ( Customer identification number in our system, the buying customer) -->
    <corp_orgno><![CDATA[5566351111]]></corp_orgno>

    <!-- Optional -->
    <corp_descr><![CDATA[The description of the company…]]></corp_descr>

    <!-- Optional, Recruiting Company URL -->
    <corp_url><![CDATA[http://recruitingcompany.com]]></corp_url>

    <!-- Optional, name of job contact visible in the ad -->
    <name><![CDATA[John Smith]]></name>

    <!-- Optional, number to job contact visible in the ad -->
    <phone><![CDATA[ 073-555 555 55 ]]></phone>

    <!-- Optional, Local ad images -->
    <image>
      <!-- Optional; Main company logo; this is used mainly if the <customerLogo> is empty-->
      <companyLogo>
        <url><![CDATA[http://www.example.com/ComapanyLogo.jpg]]></url>
      </companyLogo>
      <!-- Optional; Main company header banner, 1012X360px, visible in listing-->
      <headerLogo>
        <url><![CDATA[http://www.example.com/HeaderLogo.jpg]]></url>
      </headerLogo>
      <!-- Optional recommended use; Main company or customer logo that is visible in listing-->
      <customerLogo>
        <url><![CDATA[http://www.example.com/CustomerLogo.jpg]]></url>
      </customerLogo>
    </image>

    <!--  Optional, Local ad Media Object (Supporting youtube movie link, Webpage)
    Youtube movie and End customer www adress in the listing.
    This Will not Change the main companies Movie and www adress.
    -->
    <media>
      <video type="youtube">
        <name>Come work for us!</name>
        <url>youtube.com/watch?0=123123412</url>
      </video>
      <webpage type="webpage">
        <name>Who we are</name>
        <url>endcustomer.com/about_us</url>
      </webpage>
    </media>

    <!-- Optional, this field is reserved for companys that need to send additonal Billing Information per listing -->
    <billingInformation><![CDATA[]]></billingInformation>

    <!-- Mandatory, there must allways be either an <apply_email> or <apply_url> -->
    <apply_email><![CDATA[john@smith.com]]></apply_email>
    <apply_url><![CDATA[http://www.staffingrec.se/viewad.php?id=7018&source=Blocket]]></apply_url>

    <!-- Empty value, this node will be updated in Q4 -->
    <product_type><![CDATA[]]></product_type>

    <!-- Mandatory, these dates are for cross referencing by our customer support and have no automatic functions. -->
    <pub_date><![CDATA[2015-01-30]]></pub_date>
    <update_date><![CDATA[2015-01-30]]></update_date>
    <end_date><![CDATA[2015-03-30]]></end_date>

    <!-- Mandatory, Static information, name of Recruitment/ATS system  -->
    <provider_name><![CDATA[ ATS ]]></provider_name>

  </ad>
</ads>


